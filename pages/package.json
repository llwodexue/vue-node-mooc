    "core-js": "^3.26.0",
    "crypto-js": "^3.3.0",
    "echarts": "5.4.0",
    "element-ui": "2.15.13",
    "file-saver": "2.0.5",
    "jsencrypt": "3.3.1",
    "moment": "^2.29.4",
    "nprogress": "0.2.0",
    "quill": "1.3.7",
    "screenfull": "5.0.2",
    "sortablejs": "1.10.2",
    "vue": "2.7.14",
    "vue-count-to": "1.0.13",
    "vue-print-nb": "^1.7.4",
    "vue-router": "3.4.9",
    "vuedraggable": "2.24.3",
    "vuex": "3.6.2"
