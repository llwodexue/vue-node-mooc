    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "4.1.0",
    "compression-webpack-plugin": "5.0.2",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "1.32.13",
    "sass-loader": "10.2.0",
    "script-ext-html-webpack-plugin": "2.1.5",
    "svg-sprite-loader": "5.1.1"
